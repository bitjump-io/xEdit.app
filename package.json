{
    "private": true,
    "author": "David Neuy",
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src --outDir build --extension .fs.js --run webpack serve --env dev",
        "prebuild": "dotnet tool restore",
        "build": "dotnet fable ./src --outDir build --extension .fs.js && webpack",
        "test:live": "dotnet fable watch tests --run webpack serve --env dev --config ./webpack.tests.js",
        "build:test": "dotnet fable tests && webpack --config webpack.tests.js",
        "pretest": "dotnet tool restore",
        "test": "dotnet fable tests --outDir dist/tests && mocha dist/tests -r esm"
    },
