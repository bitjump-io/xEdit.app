{
    "private": true,
    "author": "David Neuy",
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src -o dist -e .jsx --run vite",
        "prebuild": "dotnet tool restore",
        "build": "dotnet fable ./src --outDir build --extension .fs.js && webpack",
        "test:live": "dotnet fable watch tests --run webpack serve --env dev --config ./webpack.tests.js",
        "build:test": "dotnet fable tests && webpack --config webpack.tests.js",
        "pretest": "dotnet tool restore",
        "test": "dotnet fable tests --outDir dist/tests && mocha dist/tests -r esm",
        "update-all-dependencies": "dotnet tool update fable && dotnet tool update femto && dotnet tool update dotnet-outdated-tool && dotnet outdated --upgrade ./src && node ./node_modules/npm-check/bin/cli.js --update-all"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mdi/js": "^7.1.96",
        "@mdi/react": "^1.6.1",
        "@mui/icons-material": "^5.11.0",
        "@mui/material": "^5.11.2",
        "@primer/react": "^35.16.0",
        "monaco-editor": "^0.34.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^5.3.6"
    },
    "devDependencies": {
        "@types/wicg-file-system-access": "^2020.9.5",
        "@vitejs/plugin-react": "^3.0.0",
        "@webpack-cli/serve": "^2.0.1",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "dotenv-webpack": "^8.0.1",
        "esm": "^3.2.25",
        "file-loader": "^6.2.0",
        "graceful-fs": "^4.2.10",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.7.2",
        "mocha": "^10.2.0",
        "monaco-editor-webpack-plugin": "^7.0.1",
        "npm-check": "^6.0.1",
        "react-refresh": "^0.14.0",
        "remotedev": "^0.2.9",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "terser": "^5.16.1",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4",
        "vite": "^4.0.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    }
}
