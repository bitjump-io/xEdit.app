{
    "private": true,
    "author": "David Neuy",
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src --outDir build --extension .fs.js --run webpack serve --host localhost --env dev",
        "prebuild": "dotnet tool restore",
        "build": "dotnet fable ./src --outDir build --extension .fs.js && webpack",
        "test:live": "dotnet fable watch tests --run webpack serve --env dev --config ./webpack.tests.js",
        "build:test": "dotnet fable tests && webpack --config webpack.tests.js",
        "pretest": "dotnet tool restore",
        "test": "dotnet fable tests --outDir dist/tests && mocha dist/tests -r esm",
        "update-all-dependencies": "dotnet tool update fable && dotnet tool update femto && dotnet tool update dotnet-outdated-tool && dotnet outdated --upgrade ./src && node ./node_modules/npm-check/bin/cli.js --update-all"
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@mdi/js": "^7.2.96",
        "@mdi/react": "^1.6.1",
        "@mui/icons-material": "^5.11.16",
        "@mui/lab": "^5.0.0-alpha.129",
        "@mui/material": "^5.12.3",
        "@mui/styles": "^5.12.3",
        "@mui/system": "^5.12.3",
        "monaco-editor": "^0.23.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "remotedev": "^0.2.9"
    },
    "devDependencies": {
        "@types/wicg-file-system-access": "^2020.9.1",
        "@webpack-cli/serve": "^1.3.0",
        "copy-webpack-plugin": "^8.0.0",
        "css-loader": "^5.1.1",
        "dotenv-webpack": "^7.0.1",
        "esm": "^3.2.25",
        "file-loader": "^6.2.0",
        "graceful-fs": "^4.2.6",
        "html-webpack-plugin": "^5.2.0",
        "mini-css-extract-plugin": "^1.3.9",
        "mocha": "^8.3.1",
        "monaco-editor-webpack-plugin": "^3.0.0",
        "npm-check": "^5.9.2",
        "react-refresh": "^0.9.0",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "terser": "^5.6.0",
        "ts-loader": "^8.0.17",
        "typescript": "^4.2.3",
        "webpack": "^5.24.3",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    }
}
